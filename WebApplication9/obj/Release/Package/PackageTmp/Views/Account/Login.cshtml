</div>
<div class="login-pwd">
    @{Html.RenderPartial("_ForgotPassword", new IdentitySample.Models.ForgotPasswordViewModel());}
</div>
<a href="http://wvup.edu/"><div class="logo"></div></a>

<div id="qv-pop-up"></div>

<script type="text/javascript" src="~/Scripts/_js_forgotPassClick.js"></script>
<script type="text/javascript" src="~/Scripts/_js_registerPopup.js"></script>

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}